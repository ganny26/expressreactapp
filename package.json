{
  "name": "expressreactapp",
  "version": "1.0.0",
  "main": "dist",
  "license": "MIT",
  "scripts": {
    "pord:server": "nodemon -q dist/server",
    "build": "nodemon -q ./src/server --exec babel-node --out-dir dist/server",
    "start:server": "npm run build && npm run server",
    "start:client": "webpack-dev-server --config ./internals/webpack/webpack.dev.config.js",
    "start": "concurrently \"npm run server\" \"npm run start:client\"",
    "prod": "webpack --config ./internals/webpack/webpack.prod.config.js",
    "server": "nodemon -q ./src/server/bin/www",
    "test": "jest"
  },
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@material-ui/core": "^3.9.0",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "concurrently": "^4.1.0",
    "config": "^3.0.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "sequelize": "^4.42.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.0.0",
    "nodemon": "^1.18.9",
    "react-hot-loader": "^4.6.3",
    "style-loader": "^0.23.1",
    "uuid": "^3.3.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
